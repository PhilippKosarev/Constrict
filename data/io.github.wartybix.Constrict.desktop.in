[Desktop Entry]
Name=Constrict
GenericName=Video Compressor
Comment=Compress videos to target file sizes
Exec=constrict --new-window %U
Icon=@ICON@
Terminal=false
Type=Application
Categories=AudioVideo;Video;GNOME;GTK;
Keywords=Compress;Video;Movie;Film;Clip;
StartupNotify=true
DBusActivatable=true
Actions=new-window;
X-GNOME-UsesNotifications=true
# Mime types are copy + pasted from kramo's showtime project.
# https://gitlab.gnome.org/GNOME/showtime/-/blob/d6614f70327ee251b3c62a67c15460d54fad8683/data/org.gnome.Showtime.desktop.in
MimeType=video/3gp;video/3gpp;video/3gpp2;video/dv;video/divx;video/fli;video/flv;video/mp2t;video/mp4;video/mp4v-es;video/mpeg;video/mpeg-system;video/msvideo;video/ogg;video/quicktime;video/vivo;video/vnd.divx;video/vnd.mpegurl;video/vnd.rn-realvideo;video/vnd.vivo;video/webm;video/x-anim;video/x-avi;video/x-flc;video/x-fli;video/x-flic;video/x-flv;video/x-m4v;video/x-matroska;video/x-mjpeg;video/x-mpeg;video/x-mpeg2;video/x-ms-asf;video/x-ms-asf-plugin;video/x-ms-asx;video/x-msvideo;video/x-ms-wm;video/x-ms-wmv;video/x-ms-wvx;video/x-nsv;video/x-ogm+ogg;video/x-theora;video/x-theora+ogg;

[Desktop Action new-window]
Name=New Window
Exec=constrict --new-window